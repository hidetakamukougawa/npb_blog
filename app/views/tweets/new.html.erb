<!-- メイン -->
<main>
  <!-- お問合せフォーム -->
  <div class="py-3">
    <div class="container">
      <h1 class="mb-3 text-center">新規投稿</h1>
      <!-- フォーム -->
      <form class="border w-75 p-3 mx-auto">
        <div class="form-group">
            <label for="text1">タイトル</label>
            <input type="text" class="form-control" id="text1" placeholder="タイトルを入力">
        </div>
        <div class="form-group">
          <label for="file1">画像を選択</label>
          <input type="file" class="form-control-file" id="file1">
        </div>
        <div class="form-group">
          <label for="textarea1">テキスト</label>
          <textarea class="form-control" id="textarea1" rows="8"></textarea>
        </div>
        <button type="submit" class="btn btn-primary">投稿するする</button>
      </form>
      <!-- /フォーム -->
    </div>
  </div>
  <!-- / お問合せフォーム -->
</main>
<!-- /メイン -->